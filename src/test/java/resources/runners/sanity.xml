<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="sanity Suits" verbose="1">
    <groups>
        <run>
            <include name="sanity"></include>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.orangehrmlive.demo.customListeners.CustomListeners"></listener>
        <listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
    </listeners>
    <test name="sanity">
        <classes>
            <class name="com.orangehrmlive.demo.testsuits.LoginTest"></class>
            <class name="com.orangehrmlive.demo.testsuits.UsersTest"></class>
        </classes>
    </test>
</suite>